{
	"name": "jsx-to-svg",
	"displayName": "jsx-to-svg",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jsx-to-svg.previewSvg"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jsx-to-svg.previewSvg",
				"title": "Preview as SVG"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "resourceLangId == javascript",
					"command": "jsx-to-svg.previewSvg",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.55.0",
		"babel-loader": "8.1.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "4.42.0",
		"webpack-cli": "3.3.12",
		"webpack-node-externals": "1.7.2"
	},
	"dependencies": {
		"@babel/core": "^7.14.0",
		"@babel/plugin-transform-react-jsx": "^7.13.12",
		"@babel/preset-env": "^7.14.0",
		"@babel/preset-react": "^7.13.13",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"publisher": "mertcertel",
	"repository": {
		"type": "git",
		"url": "https://github.com/yulafezmesi/jsx-to-svg"
	}
}
